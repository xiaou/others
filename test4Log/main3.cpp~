#include <stdio.h>
#include <stdlib.h>
#include <stdarg.h>
#include <iostream>

using namespace std;

#define prin(fmt, ...) printf(fmt, args)

static char buf[1024];

int prinFunc(const char * fmt, ...)
{
	va_list args;
	int n;
	va_start(args, fmt);
	n = vsprintf(buf, fmt, args);
	va_end(args);
	
	prin(buf);
	
	return n;
}

int main()
{
	int i = 44;
	prinFunc("%d\n", i);

	return 0;
}
